{
  "name": "reactjs-basics",
  "version": "1.0.0",
  "description": "Some basic ReactJS",
  "main": "index.js",
  "scripts": {
    "test": "mocha --watch --compilers js:babel-register --require babel-polyfill src/app/tests/*.js",
    "start": "npm run build",
    "build": "webpack --config webpack.config.js -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --history-api-fallback",
    "prod": "webpack --config webpack.prod.config.js -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Laurence Newman",
  "dependencies": {
    "normalizr": "^2.2.1",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.2.1",
    "react-ga": "^2.1.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-share": "^1.11.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^2.0.2",
    "zenscroll": "^3.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.1.2",
    "mocha-loader": "^1.0.0",
    "nock": "^9.0.2",
    "node-fetch": "^1.6.3",
    "redux-logger": "^2.7.0",
    "redux-mock-store": "^1.2.1",
    "sinon": "^1.17.6",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
